<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Open Customer Services" HorizontalAlignment="Center" Margin="0,64,0,0" VerticalAlignment="Top" Height="131" Width="448" Click="Button_Click"/>
        <ListBox x:Name="" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="1175,-11,-927,11" RenderTransformOrigin="0.5,0.5">
            <ListBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="1.608"/>
                    <RotateTransform/>
                    <TranslateTransform Y="2.133"/>
                </TransformGroup>
            </ListBox.RenderTransform>
        </ListBox>
        <Button Content="Mo pruduct management" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="97" Width="448"/>

    </Grid>
</Window>
